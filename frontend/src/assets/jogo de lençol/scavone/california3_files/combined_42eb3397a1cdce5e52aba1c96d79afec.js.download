		$(document).on("submit",'#storeFinderForm', function(e){
			e.preventDefault()
			var q = $(".js-store-finder-search-input").val();
			var type = $("select#pointOfServiceType").val();
			
			if(q.length>0){
				ACC.storefinder.getInitStoreData(q, null, null, type);
