  function getUserStatus() {

    //return  isLogged() ? {"useridorigin" :"SAP_HYBRIS_CONSUMER"} : {"useridorigin" :"COOKIE_ID"} ;
    return  isLogged() ? "SAP_HYBRIS_CONSUMER" : "COOKIE_ID" ;
