           	  ACC.wishlist.updateWishlistForProduct();
                   	$(".removing").slideUp(
                   			function(){
                   				$(".removing").remove();
                   			}
                   	);
                   }, 3000  );

           		return;
   			}


        }).fail(function() {
            console.log("Error trying to remove product " + code + " from the wishlist with code " + productcodelist + "!");
            $(".removing").find('.removeFromWishlist').hide();
            $(".removing").removeClass("removing");
