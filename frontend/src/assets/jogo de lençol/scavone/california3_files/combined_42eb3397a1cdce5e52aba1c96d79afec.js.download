                        success: function(data)
                        {
                            $('#wishListContent').html(data);

                        },
                        complete: function(){

                             //remove ajax flag in order to allow the next request
                             $('#wishListContent').removeClass("doingAjax");

                        },
                        error: function(data)
                        {
                        }
                        });
                      }

             	});
            },

