		{
			if((parseInt($(window).width()) <= parseInt(screenXs.replace('px',''))) && ($('body').hasClass('page-productGrid')))
			{
				$('#gridView').val('false');
				$('#listView').val('true');
				$('#sort-navigation-form').submit();
			}
			else
			{
				$.ajax({
					url: categoryResultsUrl,
					type: 'GET',
					dataType: 'html',
					data: {
						page: page,
						sort: sort,
						termo: termo,
						isGrid: isGrid
